@{
}

@model IList<HomeAccountingMVC.Entities.Account>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Firstname</th>
            <th>Username</th>
            <th>Password</th>
            <th>Email</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.ID</td>
                <td>@student.Firstname</td>
                <td>@student.Username</td>
                <td>@student.Password</td>
                <td>@student.Email</td>
                <td>
                    <a>Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>